@model IEnumerable<Ovning12Garage2._0.Models.ParkedVehicle>
<!DOCTYPE html>
<html>
<head>
<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="stylesheet" href="site.css">
<script src="site.js"></script>
<style>

body {font-family: Arial, Helvetica, sans-serif;}
</style>
</head>
<body>

<h2>Receipt for checkout - Parking Application</h2>

<button id="myButton">Pay for parking</button>

<div id="myReceipt" class="Receipt">
  <div class="receipt-content">
    <span class="close">&times;</span>
    <div class="header-receipt">
        <h3>Receipt</h3>
    </div>
    <div class="column1">
    <div class="TextColumn1">
    <h4>Parked Vehicle: @Html.DisplayNameFor(model => model.LicenseNumber) </h4>
                
      </div>
      <div class="TextColumn2">
            <h4>Time of Arrival: @Html.DisplayNameFor(model => model.TimeOfArrival)</h4>
      </div>
      <div class="TextColumn3">
            <h4>Time of Departure: </h4>
      </div>
      <div class="TextColumn4">
            <h4>Price per hour: 10 SEK</h4>
      </div>
          </div>
</div>
</div>
<script>
</script>
</body>
</html>