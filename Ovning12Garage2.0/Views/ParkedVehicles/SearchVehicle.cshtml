@using Ovning12Garage2._0.Models.Entites
 @model Ovning12Garage2._0.Models.ParkedVehicle

@{
    ViewData["Title"] = "Stoppa parkering";
}
<h1 class="display-4 text-center">Stoppa Parkering</h1>
<div class="container m-5">
    <div class="col-md-4">
        <form asp-action="SearchByReg">
                <div class="form-group">
                <label asp-for="LicenseNumber" class="control-label"></label>
                <input asp-for="LicenseNumber" class="form-control" />
                <span asp-validation-for="LicenseNumber" class="text-danger"></span>
            </div>
            <div class="form-group pt-3">
                <input type="submit" value="Sök" class="btn btn-primary" />
            </div>
             </form>
     </div>

</div>